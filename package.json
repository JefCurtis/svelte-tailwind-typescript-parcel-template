{
  "name": "svelte-template",
  "version": "1.0.0",
  "description": "A starter that includes Svelte, Parcel, Tailwind CSS, Typescript, Firebase",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html --port 5000",
    "build": "rm -rf dist && parcel build src/index.html --no-source-maps"
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "keywords": [
    "svelte",
    "parcel",
    "tailwindCSS",
    "typescript",
    "firebase"
  ],
  "author": "jefcurtis@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.8.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.6",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "svelte": "^3.24.0",
    "svelte-preprocess": "^4.0.6",
    "tailwindcss": "^1.4.6",
    "tailwindcss-font-inter": "^1.0.8",
    "typescript": "^3.9.6"
  }
}
